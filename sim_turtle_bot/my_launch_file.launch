<launch>
    <!-- Set TURTLEBOT3_MODEL environment variable -->
    <env name="TURTLEBOT3_MODEL" value="waffle_pi"/>

    <!-- Launch TurtleBot3 Manipulation MoveIt Configuration -->
    <include file="$(find turtlebot3_manipulation_moveit_config)/launch/move_group.launch"/>

    <!-- Launch Manipulator Node -->
    <node pkg="MiroSheesho" type="MiroSheesho" name="manipulator_node" output="screen"/>

    <!-- Launch Scenario Node -->
    <node pkg="scenario" type="Scenario.py" name="scenario_node" output="screen"/>

  <node name="bot" pkg="turtlebot_architecture" type="GoalSender.py" output="screen"/>
  <node name="APF" pkg="turtlebot_architecture" type="APF.py" output="screen"/>
  <node name="P2P" pkg="turtlebot_architecture" type="P2P.py" output="screen"/>
</launch>

